vbell off
scrollback 150000
defscrollback 150000
startup_message off
hardstatus string "%{= kg}[ %{G}%H %{g}] %{=}%-w%{+b w}%n %t%{-b w}%+w %{= kG}%-=%D %m/%d/%y | %C:%s %A | %1`%{-}"
caption always
caption string "%{= kg}[ %{G}%H %{g}] %{=}%-w%{+b w}%n %t%{-b w}%+w %{= kG}%-=%D %m/%d/%y | %C:%s %A | %1`%{-}"
escape ^Ee

screen -t Console 0

termcapinfo *rxvt* ti@:te@
#term rxvt-unicode-256color
